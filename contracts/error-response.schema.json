{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title": "ErrorResponse",
    "type":"object",
    "required": ["error"],
    "additionalProperties": false,
    "properties": {
        "error":{
            "type":"object",
            "required": ["code","message"],
            "additionalProperties": false,
            "properties": {
                "code":{"type":"string"},
                "message":{"type":"string"},
                "requestId":{"type":"string"}
            }
        }
    }
}